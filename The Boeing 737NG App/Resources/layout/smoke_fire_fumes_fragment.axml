                        android:layout_width="24dp" />
                    <TextView
                        android:layout_column="1"
                        android:layout_width="30px" />
                    <TextView
                        android:textSize="18dp"
                        android:layout_width="wrap_content"
                        android:layout_column="2"
                        android:text="►► Go to step 15"
